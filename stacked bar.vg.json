{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/XiaotongLi99/Assignment2/main/data/bar.csv"},
    "width": 600,
    "height": 500,
    "mark": "bar",
    "encoding": {
      "x": {
        "timeUnit": "year",
        "field": "year",
        "type": "ordinal",
        "title": "Year"
      },
      "y": {
        "field": "Enrolled students",
        "type": "quantitative"
      },
      "color": {
        "field": "state",
        "type": "nominal",
        "scale": {
          "domain": ["NSW", "VIC", "QLD", "WA", "SA", "TAS", "ACT", "NT"],
          "scheme": "set3"
        },
        "title": "States"
      }
    }
  }
  